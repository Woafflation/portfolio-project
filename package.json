{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --verbose",
    "lint": "eslint src/",
    "yarn": "yarn"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "chart.js": "^2.8.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.3.1",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.4.0",
    "react": "^16.8.3",
    "react-countup": "^4.1.3",
    "react-date-range": "^1.0.0-beta",
    "react-dom": "^16.8.3",
    "react-redux": "^7.1.0",
    "react-redux-loading-bar": "^4.2.0",
    "react-redux-toastr": "^7.5.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "react-transition-group": "^2.6.0",
    "react-use": "^10.5.0",
    "react-widgets": "^4.4.10",
    "redux": "^4.0.1",
    "redux-auth-wrapper": "^2.1.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.1.0",
